// # 1. Algorithm [Bubble Sort]
#include <stdio.h>

int main()
{
	int i,j,change;
	int num[10] = {6,1,10,8,4,3,9,7,2,5};
	
	for (i=0; i<10; i++)
	{
		for (j=0; j<i; j++)
		{
			if (num[i]<num[j])
			{
				change = num[i];
				num[i] = num[j];
				num[j] = change;
			}
		}
	}
	
	for (i=0; i<10; i++)
	{
		printf("%d ",num[i]);
	}
	
	return 0;
}

/* Time Complexity
- Best : N^2
- Average : N^2
- Bad : N^2  */
