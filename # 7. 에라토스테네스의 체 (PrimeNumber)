#include <stdio.h>

int number = 100;

int arr[101];

void primeNumber()
{
	int i,j;
	for (i=2; i<=number; i++)
	{
		arr[i] = i;
	}
	
	for (i=2; i<=number; i++)
	{
		if(arr[i]==0)
		{
			continue;
		}
		
		for (j=i+i; j<=number; j+=i)
		{
			arr[j] = 0;
		}
	}
	
	for (i=2; i<=number; i++)
	{
		if (arr[i]!=0)
		{
			printf("%d ",arr[i]);
		}
	}
}

int main()
{
	primeNumber();
	
	return 0;
}
