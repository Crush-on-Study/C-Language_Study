// # 3. Insertion Sort

#include <stdio.h>

int main()
{
	int i,j,change;
	int num[10] = {5,10,8,9,2,4,7,3,1,6};
	
	for (i=0; i<9; i++)
    {
        j=i;
        while(j>=0 && num[j]>num[j+1])
        {
            change = num[j];
            num[j] = num[j+1];
            num[j+1] = change;
            j--;
        }
    }
	
	for (i=0; i<10; i++)
	{
		printf("%d ",num[i]);
	}
	
	
	return 0;
}

/* Time Complexity
Best = N
Average = N^2
Worst = N^2  */
